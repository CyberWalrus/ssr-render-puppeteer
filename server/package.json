{
    "name": "ssr-render-puppeteer",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "nodemon --es-module-specifier-resolution=node server/index.ts",
        "build": "tsc server/*.ts --outDir build",
        "start": "node build/index.js",
        "ts:check": "tsc --noEmit",
        "eslint:check": "eslint --max-warnings 0 --ext .jsx,.js,.ts,.tsx client/",
        "eslint:fix": "eslint --fix --ext .jsx,.js,.ts,.tsx client/",
        "lint:check": "yarn eslint:check && yarn ts:check",
        "lint:fix": "yarn eslint:fix"
    },
    "dependencies": {
        "express": "4.18.2",
        "express-static-gzip": "2.1.7",
        "gzip": "0.1.0",
        "puppeteer": "22.0.0"
    },
    "devDependencies": {
        "@cyber-walrus/configs": "1.0.1",
        "@types/express": "4.17.21",
        "nodemon": "3.0.3",
        "ts-node": "10.9.2"
    }
}