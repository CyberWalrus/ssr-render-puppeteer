{
    "name": "ssr-render-puppeteer",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "start": "yarn build:client && yarn dev",
        "dev": "concurrently \"yarn preview\" \"yarn dev:server\"",
        "start:client": "concurrently \"yarn dev\" \"yarn run ts:scss:watch\"",
        "dev:client": "vite",
        "build:client": "tsc && vite build",
        "build:server": "tsc server/*.ts --outDir build",
        "start:server": "node build/index.js",
        "dev:server": "nodemon --es-module-specifier-resolution=node server/index.ts",
        "preview": "vite preview",
        "ts:scss:build": "yarn typed-scss-modules client",
        "ts:scss:watch": "yarn typed-scss-modules client --outputFolder __generated__ --watch",
        "ts:check": "tsc --noEmit",
        "eslint:check": "eslint --max-warnings 0 --ext .jsx,.js,.ts,.tsx client/",
        "eslint:fix": "eslint --fix --ext .jsx,.js,.ts,.tsx client/",
        "stylelint:check": "stylelint \"client/**/*.scss\"",
        "stylelint:fix": "stylelint \"client/**/*.scss\" --fix",
        "lint:check": "yarn eslint:check && yarn stylelint:check && yarn ts:check",
        "lint:fix": "yarn eslint:fix && yarn stylelint:fix"
    },
    "dependencies": {
        "@tanstack/react-router": "1.16.0",
        "@tanstack/router-devtools": "1.16.0",
        "clsx": "2.1.0",
        "express": "4.18.2",
        "express-static-gzip": "2.1.7",
        "gzip": "0.1.0",
        "puppeteer": "22.0.0",
        "react": "18.2.0",
        "react-dom": "18.2.0"
    },
    "devDependencies": {
        "@cyber-walrus/configs": "1.0.1",
        "@types/express": "4.17.21",
        "@types/react": "18.2.55",
        "@types/react-dom": "18.2.19",
        "@vitejs/plugin-react-swc": "3.6.0",
        "concurrently": "8.2.2",
        "critical": "7.1.0",
        "nodemon": "3.0.3",
        "ts-node": "10.9.2",
        "tsc-alias": "1.8.8",
        "typed-scss-modules": "8.0.0",
        "vite": "5.1.1",
        "vite-plugin-compression": "0.5.1"
    }
}
